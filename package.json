{
  "name": "red_tetris",
  "version": "1.0.0",
  "type": "module",
  "author": "redpelicans",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm:client-dev\" \"npm:srv-dev\"",
    "client-dev": "vite --host",
    "client-build": "vite build",
    "srv-dev": "nodemon --watch src/server src/server/main.js",
    "start": "node src/server/main.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js --fix"
  },
  "dependencies": {
    "ai-digest": "^1.5.1",
    "debug": "^4.3.5",
    "express": "^4.19.2",
    "pinia": "^2.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "vue": "^3.5.9",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^1.6.0",
    "@pinia/testing": "^0.1.5",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^24.1.1",
    "nodemon": "^3.1.10",
    "vite": "^5.4.8",
    "vitest": "^1.6.0"
  }
}
